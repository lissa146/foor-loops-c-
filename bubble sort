#include <iostream>
using namespace std;

int main() {
	int bubble[6] = { 14, 7, 1, 9, 2, 9 };
	int temp;

	for (int nox = 0; nox < 4; nox++) {

		for (int i = 0; i < 4-nox; i++) {
			if (bubble[i] > bubble[i + 1]) {
				temp = bubble[i];
				bubble[i] = bubble[i + 1];
				bubble[i + 1] = temp;

			}
			for (int k = 0; k < 5; k++)
				cout << bubble[k] << " ";
			cout << endl;
			

		}
	}
	

}
